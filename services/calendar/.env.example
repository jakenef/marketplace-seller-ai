PORT=4002
PUBLIC_BASE_URL=http://localhost:4002

# Google Calendar API Configuration
# Get these from https://console.developers.google.com/
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:4002/auth/google/callback

# Space-separated list of Google API scopes
GOOGLE_SCOPES=https://www.googleapis.com/auth/calendar.events https://www.googleapis.com/auth/calendar.readonly

# Timezone for calendar events (IANA timezone format)
# Examples: America/New_York, America/Los_Angeles, Europe/London
TIMEZONE=America/Denver
